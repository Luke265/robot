{
    "name": "robot",
    "version": "1.0.6",
    "main": "./dist/index.js",
    "exports": {
        "./cv": "./dist/cv/index.js",
        "./robotjs": "./dist/robotjs/index.js",
        "./tesseract": "./dist/tesseract/index.js",
        "./mouse": "./dist/mouse/index.js",
        "./keyboard": "./dist/keyboard/index.js",
        ".": "./dist/index.js"
    },
    "typesVersions": {
        "*": {
            "cv": [
                "dist/cv/index.d.ts"
            ],
            "mouse": [
                "dist/mouse/index.d.ts"
            ],
            "keyboard": [
                "dist/keyboard/index.d.ts"
            ],
            "robotjs": [
                "dist/robotjs/index.d.ts"
            ],
            "tesseract": [
                "dist/tesseract/index.d.ts"
            ],
            "*": [
                "dist/*.d.ts",
                "dist/index.d.ts"
            ]
        }
    },
    "scripts": {
        "install": "node dist/install/install.js"
    },
    "files": [
        "dist",
        "typings",
        "src"
    ],
    "dependencies": {
        "node-fetch": "^2.6.5",
        "tar": "^6.1.11",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@types/node": "^16.10.2",
        "@types/node-fetch": "^2.5.12",
        "@types/tar": "^4.0.5"
    }
}
